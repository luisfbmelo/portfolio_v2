  "dependencies": {
    "browserify": "^10.2.6",
    "browserify-shim": "^3.8.9",
    "jquery": "^3.5.0"
  },
  "devDependencies": {
    "grunt": "^1.0.3",
    "grunt-browserify": "^3.8.0",
    "grunt-cli": "^1.2.0",
    "grunt-concurrent": "^2.0.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-jekyll": "^0.4.2",
    "grunt-shell": "^1.1.2",
    "jquery-smooth-scroll": "^1.5.5",
    "load-grunt-tasks": "~0.6.0",
    "time-grunt": "~0.4.0"
  },
  "browser": {
    "owlcarousel": "./js/plugins/owlcarousel.js",
    "mixitup": "./js/plugins/mixitup.js",
    "ejs": "./js/plugins/ejs.js",
    "bootstrap": "./js/plugins/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "owlcarousel": {
      "exports": "owlcarousel",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "mixitup": {
      "exports": "mixitup",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "ekko-lightbox": {
      "exports": "ekko-lightbox",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "ejs": {
      "exports": "ejs"
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
